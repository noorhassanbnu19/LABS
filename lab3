#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    string name1, name2;
    int quantity1, quantity2;
    float price1, price2, total1, total2;

    cout << "Enter first product name: ";
    cin >> name1;
    cout << "Enter second product name: ";
    cin >> name2;

    cout << "Enter quantities: ";
    cin >> quantity1 >> quantity2;

    cout << "Enter prices: ";
    cin >> price1 >> price2;

    total1 = quantity1 * price1;
    total2 = quantity2 * price2;

    cout << endl;
    cout<<"--------------------------------------"<<endl;

    cout << left << setw(15) << "Item"
         << right << setw(10) << "Quantity"
         << right << setw(10) << "Price"
         << right << setw(10) << "Total" << endl;
    cout<<"--------------------------------------"<<endl;

    cout << left << setw(15) << name1
         << right << setw(10) << quantity1
         << right << setw(10) << price1
         << right << setw(10) << total1 << endl;

    cout << left << setw(15) << name2
         << right << setw(10) << quantity2
         << right << setw(10) << price2
         << right << setw(10) << total2 << endl;

    return 0;
}
