#include <iostream>
using namespace std;

int main() {
    int percentage;
    char grade;

    cout << "Enter your percentage: ";
    cin >> percentage;

    switch (percentage / 10) {
        case 10:
        case 9:
            grade = 'A';
            break;
        case 8:
            grade = 'B';
            break;
        case 7:
            grade = 'C';
            break;
        case 6:
            grade = 'D';
            break;
        default:
            grade = 'F';
            break;
    }

    cout << "Your grade is: " << grade;

    return 0;
}
